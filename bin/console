#!/usr/bin/env php
<?php
use Pars\Core\Console\Console;
use Pars\Core\Container\Container;

chdir(dirname(__DIR__));
require_once "vendor/autoload.php";
/* @var Console $console */
$console = Container::getInstance()->get(Console::class);
if ($argv[0] == './bin/console') {
    array_shift($argv);
}
$console->run($argv);